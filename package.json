{
  "name": "yourloops",
  "version": "2.0.0-alpha1",
  "private": true,
  "description": "Blip is a web app for type 1 diabetes (T1D) built on top of the [Tidepool](http://tidepool.org/) platform.",
  "repository": {
    "url": "https://github.com/mdblp/blip"
  },
  "scripts": {
    "postinstall": "lerna bootstrap",
    "mrproper": "npm run clean && lerna run mrproper",
    "postmrproper": "rm -rf node_modules cloudfront-dist/deployment/node_modules server/node_modules",
    "clean": "lerna exec -- rm -rf dist coverage deploy && lerna clean --yes",
    "postclean": "rm -rf doc dist",
    "info": "lerna info",
    "prebuild": "rm -rf dist",
    "build": "lerna --stream --scope yourloops run build",
    "postbuild": "mkdir dist && mv packages/yourloops/dist dist/static",
    "gen-lambda": "node server/cloudfront-gen-lambda.js",
    "lint": "lerna run lint",
    "test-platform-client": "lerna --stream --scope=tidepool-platform-client run test",
    "test-sundial": "lerna --stream --scope=sundial run test",
    "test-tideline": "lerna --stream --scope=tideline run test",
    "test-viz": "lerna --stream --scope=tidepool-viz run test",
    "test-blip": "lerna --stream --scope=blip run test",
    "test-yourloops": "bash -c 'source config/env.docker.sh && lerna --stream  --scope=yourloops run test'",
    "test-lambda": "bash test-lambda.sh",
    "build-yourloops": "lerna --stream --scope yourloops run build",
    "security-checks": "npm audit --production --audit-level=high --registry=https://registry.npmjs.org/",
    "prebuild-soup": "mkdir -pv doc",
    "prebuild-dev": "rm -rf dist",
    "build-dev": "NODE_OPTIONS='--max-old-space-size=4096' lerna run --stream --scope yourloops build-dev",
    "postbuild-dev": "mkdir dist && mv packages/yourloops/dist dist/static",
    "start-dev": "bash -c 'source config/env.docker.sh && lerna run --stream --scope yourloops start-dev'"
  },
  "engines": {
    "node": ">=10.x",
    "npm": ">=6.x"
  },
  "dependencies": {
    "tidepool-viz": "file:packages/viz",
    "blip": "file:packages/blip",
    "sundial": "file:packages/sundial",
    "tideline": "file:packages/tideline",
    "tidepool-platform-client": "file:packages/platform-client",
    "yourloops": "file:packages/yourloops"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.8",
    "@babel/core": "^7.12.8",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-react": "^7.12.7",
    "@babel/runtime": "^7.12.5",
    "@jsdevtools/coverage-istanbul-loader": "^3.0.5",
    "@testing-library/react-hooks": "^5.0.3",
    "@types/chai": "^4.2.14",
    "@types/enzyme": "^3.10.8",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/lodash": "^4.14.161",
    "@types/mini-css-extract-plugin": "^1.2.2",
    "@types/mocha": "^8.2.0",
    "@types/reach__router": "^1.3.6",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.7",
    "@types/sinon": "^9.0.10",
    "@types/uuid": "^8.0.0",
    "@types/webpack-subresource-integrity": "^1.2.0",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "autoprefixer": "^10.0.2",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.1",
    "babel-plugin-istanbul": "^6.0.0",
    "babel-plugin-rewire": "^1.2.0",
    "babel-preset-react-app": "^10.0.0",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "chance": "^1.1.7",
    "css-loader": "^5.0.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "eslint": "^7.18.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-lodash": "^7.1.0",
    "eslint-plugin-moment-utc": "^1.0.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "handlebars": "^4.7.6",
    "helmet": "^4.2.0",
    "html-webpack-plugin": "^4.5.0",
    "karma": "^5.2.3",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-firefox-launcher": "^2.1.0",
    "karma-mocha": "^2.0.1",
