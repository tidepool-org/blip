{
  "name": "blip",
  "version": "1.7.0-rc1",
  "private": true,
  "description": "Blip is a web app for type 1 diabetes (T1D) built on top of the [Tidepool](http://tidepool.org/) platform.",
  "repository": {
    "url": "https://github.com/mdblp/blip"
  },
  "scripts": {
    "mrproper": "rm -rf dist node_modules coverage",
    "pretest": "rm -rf coverage",
    "test": "TZ=UTC NODE_ENV=test NODE_OPTIONS='--max-old-space-size=4096' karma --single-run --log-level debug start",
    "start-dev": "NODE_ENV=development USE_WEBPACK_DEV_SERVER=true webpack-dev-server --cache --colors --port 3001 --host 0.0.0.0",
    "prebuild": "rm -rf dist",
    "build": "npm run build-app",
    "build-app": "NODE_ENV=production webpack",
    "prebuild-dev": "rm -rf dist",
    "build-dev": "NODE_ENV=development webpack",
    "lint": "eslint app test"
  },
  "engines": {
    "node": ">=10.x",
    "npm": ">=6.x"
  },
  "dependencies": {
    "@material-ui/icons": "^4.9.1",
    "@material-ui/core": "^4.11.0",
    "@hot-loader/react-dom": "^16.11.0",
    "async": "3.2.0",
    "babel-core": "^7.0.0-bridge.0",
    "body-parser": "1.19.0",
    "bows": "1.7.2",
    "browser-locale": "1.0.3",
    "classnames": "2.2.6",
    "core-js": "3.6.5",
    "create-react-class": "^15.7.0",
    "custom-protocol-detection": "1.0.1",
    "fixed-data-table-2": "0.8.15",
    "history": "3.3.0",
    "i18next": "11.9.0",
    "immutability-helper": "^3.1.1",
    "lodash": "^4.17.20",
    "moment-timezone": "^0.5.32",
    "prop-types": "15.7.2",
    "qs": "6.9.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "4.12.19",
    "react-i18next": "7.13.0",
    "react-redux": "5.1.2",
    "react-router": "3.2.5",
    "react-router-redux": "3.0.0",
    "react-scroll": "1.7.10",
    "react-select": "~2.3.0",
    "react-window-size-listener": "1.2.1",
    "redux": "3.5.2",
    "redux-cache": "0.3.0",
    "redux-devtools": "3.4.1",
    "redux-devtools-dock-monitor": "1.1.3",
    "redux-devtools-log-monitor": "1.4.0",
    "redux-immutable-state-invariant": "2.1.0",
    "redux-logger": "2.6.1",
    "redux-thunk": "2.2.0",
    "regenerator-runtime": "0.13.5",
    "reselect": "3.0.1",
    "sundial": "file:../sundial",
    "tideline": "file:../tideline",
    "tidepool-platform-client": "file:../platform-client",
    "tidepool-viz": "file:../viz"
