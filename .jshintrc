{
  "browser": true,
  "node": true,
  "bitwise": true,
  "camelcase": true,
  "eqeqeq": true,
  "eqnull": true,
  "globals": {
    "process": false,
    "require": false,
    "define": false,
    "console": false,
    "__MOCK__": false,
    "__MOCK_PARAMS__": false,
    "__UPLOAD_API__": false,
    "__API_HOST__": false,
    "__SHOW_ACCEPT_TERMS__": false,
    "__PASSWORD_MIN_LENGTH__": false
  },
  "immed": true,
  "indent": 2,
  "latedef": "nofunc",
  "noarg": true,
  "undef": true,
  "unused": false,
  "trailing": true,
  "newcap": false
}
